{
	"name": "contactly-saas",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "pnpx playwright test",
		"test:unit": "vitest",
		"test:e2e": "pnpx playwright test",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"gen:types": "pnpx supabase gen types typescript --local > src/lib/supabase-types.ts && prettier --write src/lib/supabase-types.ts",
		"seed": "tsx --tsconfig scripts/tsconfig.json scripts/seed.ts",
		"stripe:webhooks": "stripe listen --forward-to localhost:5173/api/stripe/webhook"
	},
	"dependencies": {
		"@popperjs/core": "^2.11.8",
		"@supabase/auth-helpers-sveltekit": "^0.15.0",
		"@supabase/supabase-js": "^2.91.0",
		"clsx": "^2.1.1",
		"dotenv": "^17.2.3",
		"flowbite": "^4.0.1",
		"flowbite-svelte": "^1.31.0",
		"flowbite-svelte-icons": "^3.1.0",
		"stripe": "^20.2.0",
		"svelte-french-toast": "1.2.0"
	},
	"devDependencies": {
		"@faker-js/faker": "^10.2.0",
		"@playwright/test": "^1.57.0",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-vercel": "^6.3.0",
		"@sveltejs/kit": "^2.52.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@types/detect-port": "^2.0.0",
		"@types/node": "^25.0.9",
		"@types/pg": "^8.16.0",
		"@typescript-eslint/eslint-plugin": "^8.53.1",
		"@typescript-eslint/parser": "^8.53.1",
		"autoprefixer": "^10.4.23",
		"detect-port": "^2.1.0",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.14.0",
		"pg": "^8.17.2",
		"postcss": "^8.5.6",
		"postcss-load-config": "^6.0.1",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.4.1",
		"stripe-event-types": "^3.1.0",
		"supabase": "^2.72.8",
		"svelte": "^5.51.5",
		"svelte-check": "^4.3.5",
		"svelte-preprocess": "^6.0.3",
		"sveltekit-superforms": "^2.29.1",
		"tailwindcss": "^4.1.18",
		"tslib": "^2.8.1",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"vite": "^7.3.1",
		"vitest": "^4.0.17",
		"zod": "^4.3.5"
	},
	"packageManager": "pnpm@10.28.1"
}
