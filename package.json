{
	"name": "contactly-saas",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "pnpx playwright test",
		"test:unit": "vitest",
		"test:e2e": "pnpx playwright test",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"gen:types": "pnpx supabase gen types typescript --local > src/lib/supabase-types.ts && prettier --write src/lib/supabase-types.ts",
		"seed": "tsx --tsconfig scripts/tsconfig.json scripts/seed.ts",
		"stripe:webhooks": "stripe listen --forward-to localhost:5173/api/stripe/webhook"
	},
	"dependencies": {
		"@popperjs/core": "^2.11.8",
		"@supabase/auth-helpers-sveltekit": "^0.13.0",
		"@supabase/supabase-js": "^2.90.1",
		"clsx": "^2.1.1",
		"dotenv": "^16.6.1",
		"flowbite": "^2.5.2",
		"flowbite-svelte": "^0.46.23",
		"flowbite-svelte-icons": "^1.6.2",
		"stripe": "^16.12.0",
		"svelte-french-toast": "1.2.0"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.4.1",
		"@playwright/test": "^1.57.0",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/adapter-vercel": "^5.10.3",
		"@sveltejs/kit": "^2.49.5",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@types/detect-port": "^1.3.5",
		"@types/node": "^22.19.7",
		"@types/pg": "^8.16.0",
		"@typescript-eslint/eslint-plugin": "^8.53.0",
		"@typescript-eslint/parser": "^8.53.0",
		"autoprefixer": "^10.4.23",
		"detect-port": "^1.6.1",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^9.1.2",
		"eslint-plugin-svelte": "^2.46.1",
		"pg": "^8.17.1",
		"postcss": "^8.5.6",
		"postcss-load-config": "^5.1.0",
		"prettier": "^3.8.0",
		"prettier-plugin-svelte": "^3.4.1",
		"stripe-event-types": "^3.1.0",
		"supabase": "^1.226.4",
		"svelte": "^4.2.20",
		"svelte-check": "^3.8.6",
		"svelte-preprocess": "^5.1.4",
		"sveltekit-superforms": "^2.27.4",
		"tailwindcss": "^3.4.19",
		"tslib": "^2.8.1",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"vite": "^5.4.21",
		"vitest": "^2.1.9",
		"zod": "^3.25.76"
	},
	"packageManager": "pnpm@9.7.1"
}
